// *********************************************

// UNIVERSALS

// ********************************************

:focus { 
	outline: none !important; 
	border: 0 !important 
	}
	
label {
	margin-bottom: 0px !important;
	}

h3, h4 {
	color: black;
	cursor: default;
	}	
	
h5 {
	position: relative;
	font-size: 150%;
	color: white;
	margin-top: 0;
	font-weight: $fat;
	width: 100%;
	text-align: left;
	}
	
	
// OMG... UNIVERSALS EVERYWHERES...
	
#editor-client-item-new h5,
.editor-inner-wrapper h5,
#select-basic-themes,
#advanced-color-header, 
.editor-wrapper h4 {					// q-editor under title
	text-align: center;
	margin-top: 0px;
	margin-bottom: 15px;
	opacity: 1;
	font-size: 18px;
	font-weight: 200;
	}
	
	
tr {
	background: rgba(63, 70, 82, 1);
	}
	
thead tr, thead tr th {
	border: none;
	background: transparent !important;
	}	

tr:nth-child(odd) {
	background-color: rgba(0, 0, 0, 0.15);
	}

tr:nth-child(even) {
	background-color: rgba(0, 0, 0, 0.075);
	}

tr.new {
	background-color: rgb(0,255,255) !important;
	}

// ColorThemes
th {
	background: $ecolor2;
	cursor: pointer;
	font-size: 100%;
	font-weight: $lean;
	width: 10%;
	padding: 10px;
	letter-spacing: 1px;
	padding-left: 6px;
	padding-bottom: 2px;
	}
	
td {
	padding: 5px;
	vertical-align: top;
	}
		
th.thirtyp {
	width: 30%;
	}

th.fivep { 
	width: 3%;
	padding-left: 9px;
	padding-bottom: 0;
	padding-top: 22px;
	}
	
path {											// SVG
	stroke-width: 1.5px;
	}
		
path:hover {									// ditto
	fill-opacity: .7;
	}
	
a {
	font-weight: $median;
	}
	
.active a,										// Link Color
.editor-wrapper a {
	color: cadetblue !important;
	font-weight: 900 !important;
	text-shadow: -1px 1px 1px rgba(255,255,255,0.3);
	}	
		
	
		
// *********************************************
	
// Handy hooks

// *********************************************
	
	
.box {
	margin: 4px !important;
	}	

.die {
	display: none !important;
	}

.show {
	display: block;
	}

.relative {
	position: relative;
	}	

.inline {
	display: inline-block;
	vertical-align: top;
	}
	
.inliner {
	position: absolute;
	right: 1px;
	top: 8px;
	}

.underline {
	text-decoration: underline;
	}				

.search {										// Search fields
	margin: 0px;
	border: none;
	padding: 5px;
	font-family: $font;
	font-weight: $lean;
	color: black;
	background: white;
	font-size: 120%;
	padding-left: 11px;
	width: 171px;
	text-align: left;
	position: relative;
	box-shadow: 0 0 3px 0 rgba(0,0,0,0.4) inset;
	}		
	
.eightyWidth {									// Often used in tables
	background: rgba(0, 0, 0, 0.3);
	color: white !important;
	border: 0;
	box-shadow: none;
	border-radius: 0;
	padding: 6px !important;
	height: 30px !important;
	margin-bottom: 10px !important;
	margin-top: 0 !important;
	}




// ********** The Gray Background + Input Fields **********

.editor-inner-wrapper {							// The Gray background overlaying in menu...
	padding: 5px;
	margin-bottom: 3px;
	position: relative;
	margin-top: 8px;
	border-radius: 0px;
	cursor: pointer;
	padding-left: 20px;
	padding-right: 20px;
	background-color: rgba(55, 55, 75, 0.15);
	width: 230px;	
	}	

.editor-inner-wrapper:hover {
	background-color: rgba(55, 55, 75, 0.25);
	}	
	
.editor-inner-wrapper h5 {						// Client : New Client title
	padding: 0;
	position: relative;
	padding-top: 10px;
	}

.editor-inner-wrapper input, 					// Input fields in gray wrapper
.input-box {
	padding: 7px;
	color: white;	
	font-weight: $lean;
	background: rgba(0, 0, 0, 0.7);
	}

.input-box {									// Input fields in "Connect to MapBox" and "Upload Layers"
	float: left;
	width: 170px;
	border: 0;
	}
	
.search {
	padding: 5px !important;
	}


