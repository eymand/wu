.smap-button-white,
.smap-button-gray {							// Button : standard WHITE button
	height: 30px;
	text-align: center;
	margin: 0;
	margin-bottom: 10px;
	padding: 0px 10px 0px 10px;
	border: 0px;
	border-radius: 0;
	line-height: 2.1;
	font-family: $font;
	font-size: 110%;
	font-weight: $median;
	cursor: pointer;
	position: relative;
	display: inline-block;
	vertical-align: top;
	background: rgba(255,255,255,0.75);
	box-shadow: -1px 1px 2px 0 rgba(0, 0, 0, 0.3);
	color: #666;
	}

.smap-button-gray {
	background-color: rgba(0, 0, 0, 0.65);
	color: white;
	font-weight: 500;
}	

.smap-button-white:active,
.smap-button-gray:active {
	box-shadow: -1px 1px 2px 0 rgba(0, 0, 0, 0.0);
	}
	
.smap-button-white:hover {
	background: white;
	color: #333;
	}

.smap-button-gray:hover {
	background: black;
}	
	
.smap-button-white.small {						// Button : standard WHITE button smaller
	height: 26px;
	font-size: 13px;
	line-height: 2;
	}	
	
.smap-button-white.small.red {					// Button : red button
	color: white !important;
	background: #B80077 !important;
	box-shadow: 0 0 0 1px white inset;
	}
	
.item-list {									// List Buttons (map layers, controls, etc)
	height: 32px;
	margin-bottom: 10px;
	color: rgba(255,255,255,1);
	cursor: pointer;
	border: 0px;
	line-height: 1;
	border-radius: 0;
	font-family: $font;
	font-size: 100%;
	font-weight: $lean;
	position: relative;
	margin-bottom: 1px;
	width: 100%;
	padding: 0px;
	opacity: .7;
	background: rgba(0, 0, 0, 0.70);

	-webkit-transition: height 150ms ease;
	-moz-transition: height 150ms ease;
	-o-transition: height 150ms ease;
	transition: height 150ms ease;	
	}

.item-list-header {
	margin-top: 10px;
	margin-bottom: 3px;
	}

.item-list div,
.layers-mapbox-item,
.select-elem {
	/*padding: 8px;*/
	}	

.item-list-inner-text {
	position: absolute;
	width: 100%;
	vertical-align: top;
	display: inline-block;
	margin: auto;
	line-height: 1;
	top: 0;
	bottom: 0;
	font-size: 12px;
	font-weight: 500;
	padding: 0px;
	overflow: hidden;
	max-height: 25px;
	padding-left: 8px;
	padding-right: 8px;
	}
	
.item-list:hover {
	opacity: 1;
	background: rgba(255, 255, 255, 0.5);
	color: black;
	}
	
.item-list, .layer-item, .layers-mapbox-item {
	color: #333;
	/*background: rgba(255,255,255,0.75);*/
	/*box-shadow: -1px 1px 2px -1px rgba(0, 0, 0, 0.6) inset, 1px -1px 2px -1px white inset;*/
	/*background: white;*/

	color: #666;
	background: rgba(255, 255, 255, 0.4);
	opacity: 1;
	/*box-shadow: 0 0 0px 1px rgba(255,255,255,0.5) inset;	*/

	}

.item-list.active,
.layer-item.active,
.active .layers-mapbox-item {								// List Buttons : Active State
	background-color: white;
	color: black;
	font-weight: 400;	
	opacity: 1;

