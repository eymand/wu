// *********************************************
		
// General

// *********************************************

body {
	width: 100%;
	height: 100%;
	overflow: hidden;
	background-color: #ebeced;
	color: $ecolor2;
	line-height: 1.4;
	font-family: $font;
	font-size: 14px;
	text-align: left;
	
	-webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    }
	
#app {									// The Outer Super Wrapper	
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 200;
	}
	
#map {									// Map
	position: absolute;
	width: 100%;
	height: 100%;
	height: 100%;
	left: 0px;
	-webkit-transition: -webkit-filter 250ms linear, opacity 250ms linear !important;
	-moz-transition: -moz-filter 250ms linear, opacity 250ms linear !important;
	transition: filter 250ms linear, opacity 250ms linear !important;
	}



.click-to-start:after {
	position: relative;
	width: 100%;
	height: 100%;
	left: 36px;
	top: 93px;
	text-align: center;
	display: block;
	font-size: 42px;
	color: #666;
	font-weight: 200;
	content: '';
	padding-right: 0px;
	position: relative;
	background-image: url(images/click2start.png);
	background-repeat: no-repeat;

	
	}
	
#map:before {
	content: '';
	background-image: url('images/systemapic_BG.jpg');
	position: absolute;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	}

.g-body {								// This holds tooltop etc.
	z-index: 35000;	
	top: 5px;
	left: -17px;
	position: relative;
	}
	
#header {								// The Header
	position: absolute;
	height: 77px;
	z-index: 10;
	top: 6px;
	left: 104px !important;
	width: 100% !important; // To override Knut
	padding-right: 110px;
	}

#headerResizer { 						// The Header
	position: absolute;
	bottom: 0px;
	width: 100%;
	height: 6px;
	cursor: ns-resize;
	}

.headerResizerDisabled { 				// The Header
	cursor: default !important;
	}

#header .header-logo {					// The Header
	position: relative;
	z-index: 1000;
	color: white;	
	float: left;
	max-height: inherit;
	padding: 5px;
	margin-right: 10px;

	// To override Knut
	float: left !important;
	position: absolute !important;	
	background-repeat: no-repeat !important;
	background-position: center !important;
	left: 3px !important;
	background-color: transparent !important;
	background-size: 100% 100% !important;
	top: 3px !important;
	height: 71px !important;
	}

// To override Knut
.header-title-wrap {
	background: rgba(255, 255, 255, 0.93) !important; // To override Knut
	height: 100% !important; // To override Knut
	width: 100% !important; // To override Knut
	top: 0 !important; // To override Knut
	box-shadow: 0 0 5px 0 rgba(0,0,0,0.3);
	}

.header-logo img {						// The Header
	max-height: 100%;
	padding-bottom: 10px;
	width: auto;
	}

.header-title {							// The Header
	border: 0px;
	background: transparent;
	font-size: 30px;
	font-weight: $lean;
	padding-left: 102px;
	}

.header-title input {
	padding: 0;
	/*padding-left: 102px;*/
	width: 100%;
	/*background: red;*/
	}



.header-subtitle {						// The Header
	border: 0px;
	background: transparent;
	display: block;
	font-style: italic;
	color: #666;
	font-size: 16px;
	font-weight: $lean;
	float: left;
	height: 55px;
	word-wrap: break-word;
	position: relative !important;
	left: inherit;
	top: 0 !important;
	line-height: 15px;
	overflow: hidden;
	max-height: 31px;
	padding-right: 14px;
	padding-left: 102px;
	}

.header-subtitle input {
	/*margin-top: 7px;*/
	width: 300px;
	color: black;
	/*padding-left: 102px;*/
	width: 100%;
	/*background: red;*/
	margin: 0 !important;
	padding: 0 !important;
	left: 0 !important;
	top: -9px !important;
	/*position: relative;*/
	
	position: absolute;
	}	

#header input {							// The Header
	outline: none;
	}
 
#header input:focus  {					// The Header
	border: 1px solid rgb(81, 203, 238);
	}
	

// *********************************************

// THE Q-EDITOR --> The Menu on the Left

// *********************************************
	
.q-editor-container {
	position: absolute;
	overflow: hidden;
	z-index: 2000;	
	-webkit-transition: height 250ms ease, width 250ms ease;
	-moz-transition: height 250ms ease, width 250ms ease;
	transition: height 250ms ease, width 250ms ease;

	width: 95px;

	background-color: rgba(255,255,255,0.0);
	box-shadow: none;
	top: 84px !important; // To override Knut
	left: 4px;
	padding-top: 2px;
	height: 80px;
	}

.disabled {
	opacity: 0.1;
	height: 0px !important;
	}	

.disabled a {
	opacity: 0;
	}	
	
/*// ColorThemes */
.q-editor-menu {
	margin: 0;
	padding: 0;
	position: absolute;
	box-shadow: 0 0px 5px 0 rgba(0,0,0,0.3) !important;
	margin-top: 7px;
	height: auto;
	left: 2px;
	width: 90px;	
	background-color: rgba(255, 255, 255, 0.95);
	top: 79px;
	top: 6px !important; // To override Knut
	}

.q-editor-menu-item {
	text-align: center;
	height: 70px;
	position: relative;
	cursor: pointer;
	z-index: 200;
	}

.q-editor-menu-item a {
	width: 100%;
	font-size: 90%;
	text-transform: uppercase;
	font-weight: $fat;
	bottom: 0px;
	position: absolute;
	left: 0px;
	top: 0;
	height: 100%;
	line-height: 5;
	text-decoration: none;
	color: #666;
	line-height: 1;
	padding-top: 20px;
	}

.q-editor-menu-item a:hover {
	color: #333;
	}	

.q-editor-content-item {
	position: absolute;
	left: 100px;
	display: none;
	padding: 10px;
	overflow-y: auto;
	overflow-x: hidden;
	opacity: 1;
	background-color: rgba(255,255,255,0.90);
	margin-top: 20px;
	box-shadow: 0 0 5px 0 rgba(0,0,0,0.3) !important; // To override Knut
	height: auto;
	margin-left: 0px;
	margin-right: 20px;
	width: 250px;
	margin-top: 86px;
	max-height: 70%;
	top: 6px !important; // To override Knut
	}

// ColorThemes .ct1
.editor-wrapper { 						// The inner Wrapper of the .q-editor-content-item
	height: 100%;
	overflow-y: scroll;
	padding-top: 0;
	padding-bottom: 0;
	padding-left: 0px;
	color: black;
	width: 260px;
	padding-right: 30px;	
	}

.editor-wrapper h3 {					// q-editor main title
	text-align: center;
	font-weight: $lean;
	margin-top: 0;
	font-size: 220%;
	}

#menuslider {							// The background slider
	height: 70px;
	top: 0px;
	position: absolute;	
	width: 90px;
	background-color: rgba(0, 0, 0, 0.075);

	background-color: rgba(0, 0, 0, 0.05);
	/*border-top: 1px dotted rgba(0,0,0,0.3);*/
	/*border-bottom: 1px dotted rgba(0,0,0,0.3);*/

	}

#menuslider-arrow {
	position: absolute;
	right: -9px;
	width: 9px;
	height: 72px;
	overflow: hidden;
	}

#menuslider-arrow:before { 
	content: '';
	position: absolute;
	height: 30px;
	width: 30px;
	top: 21px;
	background-color: #e9e9e9;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	transform: rotate(45deg);
	left: -29px;
	box-shadow: 0 0 5px 0 rgba(0,0,0,0.075);
	z-index: 999;
	}

#menucloser {							// The menu pane closer
	position: absolute;
	bottom: 20px;
	left: 40px;
	background: white;
	width: 19px;
	height: 19px;
	z-index: 99999;
	border-radius: 20px;
	cursor: pointer;
	background-image: url(images/evil_sprite.png);
	background-repeat: no-repeat;
	background-position: 4px 8px;
	-webkit-transform: rotate(270deg);
	-moz-transform: rotate(270deg);
	transform: rotate(270deg);
	}
	
.pane-open {							// The menu pane closer in open state
	-webkit-transform: rotate(90deg) !important;	
	-moz-transform: rotate(90deg) !important;	
	transform: rotate(90deg) !important;	
	}	

